{
  "npmScope": "thrive",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "lint-styles",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "assets": {
      "tags": ["app"]
    },
    "dashboard-grid": {
      "tags": ["app"]
    },
    "thrive": {
      "tags": ["app"]
    },
    "thrive-e2e": {
      "tags": ["test"],
      "implicitDependencies": ["thrive"]
    },
    "nav-bar": {
      "tags": ["app"]
    },
    "shared-ui": {
      "tags": ["shared"]
    }
  }
}
