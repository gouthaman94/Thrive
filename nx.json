{
  "npmScope": "thrive",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e", "lint-styles"]
      }
    }
  },
  "projects": {
    "dashboard-grid": {
      "tags": ["app"]
    },
    "iron-hide": {
      "tags": ["app"]
    },
    "iron-hide-e2e": {
      "tags": ["test"],
      "implicitDependencies": ["iron-hide"]
    },
    "nav-bar": {
      "tags": ["app"]
    },
    "shared-ui": {
      "tags": ["shared"]
    }
  }
}
