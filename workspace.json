{
  "version": 2,
  "projects": {
    "assets": {
      "root": "libs/assets",
      "sourceRoot": "libs/assets/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/assets/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/assets"
          ],
          "options": {
            "jestConfig": "libs/assets/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dashboard-grid": {
      "root": "libs/dashboard-grid",
      "sourceRoot": "libs/dashboard-grid/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/dashboard-grid/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/dashboard-grid"
          ],
          "options": {
            "jestConfig": "libs/dashboard-grid/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "iron-hide": {
      "root": "apps/iron-hide",
      "sourceRoot": "apps/iron-hide/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/iron-hide",
            "index": "apps/iron-hide/src/index.html",
            "main": "apps/iron-hide/src/main.tsx",
            "polyfills": "apps/iron-hide/src/polyfills.ts",
            "tsConfig": "apps/iron-hide/tsconfig.app.json",
            "assets": [
              "apps/iron-hide/src/favicon.ico",
              "apps/iron-hide/src/assets"
            ],
            "styles": [
              "apps/iron-hide/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/iron-hide/src/environments/environment.ts",
                  "with": "apps/iron-hide/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "iron-hide:build",
            "hmr": true,
            "webpackConfig": "webpack.config.js"
          },
          "configurations": {
            "production": {
              "buildTarget": "iron-hide:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/iron-hide/**/*.{ts,tsx,js,jsx}"
            ],
            "fix": true
          }
        },
        "lint-styles": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx stylelint apps/iron-hide/src/**/*.scss"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/iron-hide"
          ],
          "options": {
            "jestConfig": "apps/iron-hide/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "iron-hide-e2e": {
      "root": "apps/iron-hide-e2e",
      "sourceRoot": "apps/iron-hide-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/iron-hide-e2e/cypress.json",
            "tsConfig": "apps/iron-hide-e2e/tsconfig.e2e.json",
            "devServerTarget": "iron-hide:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "iron-hide:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/iron-hide-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "nav-bar": {
      "root": "libs/nav-bar",
      "sourceRoot": "libs/nav-bar/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/nav-bar/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "lint-styles": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx stylelint libs/nav-bar/src/**/*.scss"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/nav-bar"
          ],
          "options": {
            "jestConfig": "libs/nav-bar/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-ui": {
      "root": "libs/shared-ui",
      "sourceRoot": "libs/shared-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared-ui/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared-ui"
          ],
          "options": {
            "jestConfig": "libs/shared-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/shared-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/shared-ui",
            "config": {
              "configFolder": "libs/shared-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint",
        "strict": true
      }
    }
  },
  "defaultProject": "iron-hide"
}